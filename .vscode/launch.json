    // IntelliSense を使用して利用可能な属性を学べます。
    // 既存の属性の説明をホバーして表示します。
    // 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: plot_dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/plot_dataset.py",
            "args": [
                "${workspaceFolder}/data/train"
            ]
        },        
        {
            "name": "Python: plot_estimating_time",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/plot_estimating_time.py",
            "args": [
                "10000",
                "\"plot_estimating_time\"",
                "--NN=MnasNet",
                "--pytorch-model-file=${workspaceFolder}/result/pytorch/lastest.model",
                "-g0"
            ]
        },              
        {
            "name": "Python: plot_training_time",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/plot_training_time.py",
            "args": [
                "\"plot_training_time\"",
                "--pytorch-log=${workspaceFolder}/result/log.txt"
            ]
        },        
        {
            "name": "Python: tran_restart",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train.py",
            "args": [
                "pytorch",
                "-g0",
                "--NN=MnasNet",
                "--batchsize=64",
                "--data-augmentation",
                "--use-visibility",
                "--out=${workspaceFolder}/result",
                "--opt=Adam",
                "-e5000"
            ]
        },
        {
            "name": "Python: tran",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/train.py",
            "args": [
                "pytorch",
                "-g0",
                "--NN=MnasNet",
                "--batchsize=64",
                "--resume-model=${workspaceFolder}/result/pytorch/lastest.model",
                "--out=${workspaceFolder}/result",
                "--data-augmentation",
                "--use-visibility",
                "--opt=Adam",
                "-e5000"
            ]
        },
        {
            "name": "Python: dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/dataset.py"
        },
        {
            "name": "Python: pytorch2coreml",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/pytorch2onnx.py",
            "args": [
                "--NN=MnasNet",
                "--input=result/pytorch/lastest.model",
                "--onnx_output=result/onnx_output.onnx",
                "--output=result/MobileNet_.mlmodel"
            ]
        },        
        {
            "name": "Python: onnx_check",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/onnx_check.py",
        },        
        {
            "name": "Python: mmdnn_convert",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/mmdnn_convert.py",
            "args": [
                "--srcFramework=pytorch",
                "--NN=MobileNet_3",
                "--inputShape=3,244,244",
                "--resume-model=${workspaceFolder}/result/pytorch/epoch-1.model",
                "-d${workspaceFolder}/result/non_state.model",
                "-n${workspaceFolder}/result/non_state.model",
            ]
        },        
        {
            "name": "Python: mmdnn_conversion",
            "type": "python",
            "request": "launch",
            "program": "c:/users/yukih/appdata/local/conda/conda/envs/tensorflow-gpu/lib/site-packages/mmdnn/conversion/_script/convertToIR.py",
            "args": [
                "-fpytorch",
                "-dnon_state.model",
                "-nnon_state.model",
            ]
        },        
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "port": 3000,
            "secret": "my_secret",
            "host": "localhost"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (external)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Django"
            ]
        },
        {
            "name": "Python: Flask (0.11.x or later)",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "${workspaceFolder}/app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ]
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "module.name"
        },
        {
            "name": "Python: Pyramid",
            "type": "python",
            "request": "launch",
            "args": [
                "${workspaceFolder}/development.ini"
            ],
            "debugOptions": [
                "RedirectOutput",
                "Pyramid"
            ]
        },
        {
            "name": "Python: Watson",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/console.py",
            "args": [
                "dev",
                "runserver",
                "--noreload=True"
            ]
        },
        {
            "name": "Python: All debug Options",
            "type": "python",
            "request": "launch",
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "module": "module.name",
            "env": {
                "VAR1": "1",
                "VAR2": "2"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "arg1",
                "arg2"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}